<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/switch_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/switch_user" />

    <ImageView
        android:id="@+id/currency_dollar_red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/currency_dollar_red" />

    <ImageView
        android:id="@+id/cookie_128"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cookie_128" />

    <ImageView
        android:id="@+id/contact_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/contact_new" />

</LinearLayout>
